<div id="<%= dom_id pantry_item %>">
  <p>
    <strong>Name:</strong>
    <%= pantry_item.name %>
  </p>

  <p>
    <strong>Barcode:</strong>
    <%= pantry_item.barcode %>
  </p>

  <%= image_tag pantry_item.img_url %>

  <p>
    <strong>User:</strong>
    <%= pantry_item.user_id %>
  </p>

</div>
