<div id="<%= dom_id pantry_item %>">
  <p>
    <strong>Name:</strong>
    <%= pantry_item.name %>
  </p>

  <p>
    <strong>Barcode:</strong>
    <%= pantry_item.barcode %>
  </p>

  <img src="<%= pantry_item.img_url%>" alt="Product Image">

  <p>
    <strong>User:</strong>
    <%= pantry_item.user_id %>
  </p>

</div>
